#needsharebutton-postbottom {
    position: relative;
    cursor: pointer;
    height: 26px;
    .btn {
        display: inline-block;
        border: 1px solid rgba(255, 255, 255, 0.8);
        border-radius: 999em;
        padding: 0 10px;
        color: #ffffff;
        line-height: 24px;
        font-size: 12px;
        text-decoration: none;
        margin: 0 1px;
        margin-bottom: 6px;
    }
    .need-share-button_dropdown {
        img.need-share-wechat-code-image {
            max-width: 150px;
            margin-bottom: 20px;
        }
    }
}

#needsharebutton-postbottom.header-share {
    .btn:hover {
        color: white;
        border-color: white;
        background-color: rgba(255, 255, 255, 0.4);
        text-decoration: none;
    }
    .need-share-button_dropdown {
        color: white;
        border-color: white;
        background-color: rgba(255, 255, 255, 0.2);
    }
}

#needsharebutton-postbottom.post-share {
    .btn {
        color: #808080;
        margin-bottom: 0px;
        border: none;
    }
    .btn:hover {
        color: #0085a1;
    }
    .need-share-button_dropdown {
        color: white;
        background-color: rgba(64, 64, 64, 0.9);
    }
}


